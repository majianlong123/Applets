/* 分类 */
.fenlei {
  display: flex;
  width: 750rpx;
  margin: 20rpx auto;
  font-size: 24rpx;
  overflow-y: scroll;
  flex-wrap: wrap;
}

.fenlei>view {
  margin-bottom: 32rpx;
  margin-left: 32rpx;
  width: 20%;
  align-items: center;
}

.fenlei image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-bottom: 22rpx;
}

.img {
  width: 100%;
  height: 100%;
}

/*秒杀专区 */
.clean {
  width: 710rpx;
  font-size: 30rpx;
  margin: 20rpx auto;
  background-color: #fff;
  border-radius: 20rpx;
}

.clean .top {
  border-radius: 20rpx;
  position: relative;
  top: -20rpx;
  left: 0;
}

.clean .top image {
  width: 100%;
  height: 130rpx;
}

.clean .top view {
  position: absolute;
  top: 10rpx;
  right: 60rpx;
  text-align: right;
  padding: 45rpx;
  color: #ffffff;
}

.clean .boot {
  width: 710rpx;
  display: flex;
  justify-content: flex-start;
  margin: 0rpx auto;
}

.clean .left {
  width: 330rpx;
  border-radius: 20rpx;
  position: relative;
  top: 8rpx;
  right: 0;
}

.clean .right {
  width: 330rpx;
  border-radius: 20rpx;
  position: relative;
  top: 10rpx;
  left: 0;
}

.clean .right image {
  width: 100%;
  height: 130rpx;
}

.clean .left image {
  width: 105%;
  height: 160rpx;
  position: absolute;
  top: -32rpx;
  left: 32rpx;
}

.clean .left view {
  padding: 45rpx;
  color: #ffffff;
  position: absolute;
  top: 0;
  left: 22rpx;
}

.clean .right view {
  padding: 45rpx;
  color: #ffffff;
  position: absolute;
  top: 0;
  left: 20rpx;
}

/* 家政 */
.house {
  margin: 20rpx auto;
  display: flex;
  background: #ffffff;
}

.house .left {
  width: 45%;
  height: 345rpx;
  position: relative;
}

.house .left .title {
  font-size: 34rpx;
  font-weight: bold;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #1db0fd;
  border-radius: 20rpx;
  padding: 15rpx;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}

.house .left image {
  width: 250rpx;
  height: 230rpx;
  padding-top: 102rpx;
  padding-left: 20rpx;
}

.house .left .subtitle {
  font-size: 25rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #c3c3c3;
  position: absolute;
  top: 90rpx;
  left: 30rpx;
}

.house .right {
  width: 55%;
  border-radius: 20rpx;
  padding: 10rpx;
  position: relative;
}

.house .right .top {
  height: 50%;
  border-bottom: 1rpx solid #dfdfdf;
}

.house .right .top image {
  width: 150rpx;
  height: 100rpx;
  position: absolute;
  top: 25rpx;
  right: 0;
}

.house .right .title {
  font-size: 34rpx;
  font-weight: bold;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #fb5453;
  border-radius: 0 20rpx 0 0;
  padding: 20rpx 0 0 20rpx;
}

.house .right .top .subtitle {
  font-size: 25rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #c3c3c3;
  padding: 10rpx 20rpx;
}

.house .right .under {
  height: 50%;
  position: relative;
}

.house .right .under image {
  width: 150rpx;
  height: 100rpx;
  position: absolute;
  top: 25rpx;
  right: 0;
}

.house .right .under .title {
  font-size: 34rpx;
  font-weight: bold;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #fb5453;
  border-radius: 0 20rpx 0 0;
  padding: 20rpx 0 0 20rpx;
}

.house .right .under .subtitle {
  font-size: 25rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #c3c3c3;
  padding: 10rpx 20rpx;
}

/* 推荐商品 */
.recom {
  width: 710rpx;
  position: relative;
  margin: 30rpx auto;
  margin-bottom: 60rpx;
}

.recom .label {
  position: absolute;
  top: 20rpx;
  width: 155rpx;
  font-size: 30rpx;
  padding-top: 20rpx;
  padding: 15rpx;
  background-color: #4e51de;
  box-shadow: 0px 2px 5px 0px rgba(51, 54, 198, 0.35);
  border-radius: 0rpx 50rpx 50rpx 0rpx;
  color: white;
}

.recom .cont {
	box-shadow: 0px 1px 4px 0px rgba(192, 192, 192, 0.55);
  padding: 110rpx 0rpx 20rpx 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  border-radius: 20rpx;
  background: #fff;
}

.recom .cont .a {
  width: 140rpx;
  padding: 5rpx 45rpx;
}

.recom image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 20rpx;
}

.recom .title {
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #333333;
}

.recom .price {
  font-size: 25rpx;
  color: #ff5958;
  padding: 10rpx 0;
}





